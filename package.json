{
	"name": "fast-style",
	"displayName": "fast-style",
	"description": "fast style allow quickly edit style for angular or basic html app",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.comment",
				"title": "Comment current line"
			},
			{
				"command": "extension.trakingUserInput",
				"title": "Tracking user input"
			},
			{
				"command": "extension.sendCode",
				"title": "Sending Code To Server"
			},
			{
				"command": "extension.getInfo",
				"title": "Get File Info"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"connect": "^3.7.0",
		"cors": "latest",
		"express": "^4.17.1",
		"node-sass": "^4.12.0",
		"rxjs": "^6.5.2",
		"sass.js": "^0.11.0",
		"socket.io": "^2.2.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.5",
		"@types/express": "^4.17.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/node-sass": "^4.11.0",
		"@types/socket.io": "^2.1.2",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.5.3",
		"vscode-test": "^1.0.0-next.0"
	}
}
